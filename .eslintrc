{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "globals": {
    "__DEV__": true,
    "VERSION": true
  },
  "env": {
    "browser": true,
    "mocha": true
  },
  "rules": {
    "no-confusing-arrow": 0,
    "react/jsx-quotes": 0,
    "object-shorthand": 0,
    "jsx-quotes": [
      2,
      "prefer-double"
    ],
    "react/prop-types": 0,
    "react/jsx-closing-bracket-location": 0,
    "prefer-template": 0,
    "max-len": 0,
    "radix": 0,
    "react/sort-comp": 0,
    "jsx-a11y/img-has-alt": 0,
    "global-require": 0,
    "no-console": 0,
    "no-unused-expressions": 0,
    "quote-props": 0,
    "arrow-body-style": 0,
    "dot-notation": 0,
    "no-param-reassign": 0,
    "no-unused-vars": 0,
    "no-case-declarations": 0,
    "consistent-return": 0,
    "prefer-rest-params": 0,
    "no-nested-ternary": 0,
    "func-names": 0,
    "space-before-function-paren": 0
  }
}
